{
  "name": "performative-ts",
  "version": "1.0.0",
  "description": "Algebraic effects inspired, composable, minimalist and typed injection library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --coverage",
    "test-dev": "jest --watch",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "algebraic effects",
    "injection",
    "typed",
    "typescript",
    "functional",
    "composable"
  ],
  "author": "GÃ©raud Henrion <geraud.henrion@gmail.com>",
  "license": "MIT",
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  },
  "devDependencies": {
    "@cycle/time": "^0.20.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-jest": "^26.5.2",
    "eslint": "^7.11.0",
    "jest": "^26.6.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
